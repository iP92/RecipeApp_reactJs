{
  "name": "react-recipes",
  "version": "1.0.0",
  "description": "A starter for React-Apollo-GraphQL projects",
  "main": "server.js",
  "engines": {
    "node": "v13.0.1",
    "npm": "6.12.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-server-express": "^1.3.6",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.9",
    "react": "^16.11.0",
    "react-pose": "^4.0.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "apollo-boost": "^0.4.7",
    "nodemon": "^1.19.4"
  }
}
